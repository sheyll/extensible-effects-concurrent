{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.stack-work": true,
    "**/.build": true,
    "**/dist": true,
    "**/dist-newstyle": true,
    "**/.idea": true,
    "**/.idea*": true,
    "**/.DS_Store": true,
    "**/*.elmi": true,
    "**/*.elmo": true,
    "**/*.hi": true,
    "**/*.o": true,
    "**/elm-stuff": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.stack-work/**": true,
    "**/.build/**": true,
    "**/dist/**": true,
    "**/dist-newstyle/**": true,
    "**/.idea/**": true,
    "**/bower_components": true,
    "**/elm-stuff/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.stack-work": true,
    "**/.build": true,
    "**/dist": true,
    "**/dist-newstyle": true,
    "**/.idea": true,
    "**/.idea*": true,
    "**/.DS_Store": true,
    "**/*.elmi": true,
    "**/*.elmo": true,
    "**/*.hi": true,
    "**/*.o": true
  },
  "files.trimFinalNewlines": true,
  "editor.tabSize": 2,
  "nixEnvSelector.nixShellConfig": "${workspaceRoot}/shell.nix"
}
