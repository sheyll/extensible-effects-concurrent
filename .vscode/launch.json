{
  "version": "0.2.0",
  "configurations": [{

    "type": "ghc",
    "name": "haskell-debug-adapter",
    "request": "launch",
    "internalConsoleOptions": "openOnSessionStart",
    "workspace": "${workspaceRoot}",
    "startup": "${workspaceRoot}/test/Debug.hs",
    "startupFunc": "debugMain",
    "startupArgs": "",
    "stopOnEntry": true,
    "mainArgs": "",
    "ghciPrompt": "H>>= ",
    "ghciInitialPrompt": "Î» ",
    "ghciCmd": "stack ghci --test --no-load --no-build --main-is extensible-effects-concurrent:test:extensible-effects-concurrent-test --ghci-options -fprint-evld-with-show",
    "ghciEnv": {},
    "logFile": "${workspaceRoot}/.vscode/phoityne.log",
    "logLevel": "WARNING"
  }],
  "compounds": []
}
